{
  "name": "hns-stake-server",
  "version": "0.0.0",
  "main": "src/server.ts",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev -r tsconfig-paths/register -r dotenv/config --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "build": "babel src --extensions \".ts\" --out-dir dist --copy-files --no-copy-ignored",
    "test": "jest",
    "typeorm": "ts-node node_modules/typeorm/cli.js",
    "docker:db:start": "docker run -d --name hns-stake-db -e POSTGRES_PASSWORD=docker -e POSTGRES_DB=hns-stake -p 5432:5432 postgres:10.15",
    "db:seed": "NODE_PATH=./src ts-node -r dotenv/config src/db/seed.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^8.0.29",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv": "^14.3.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "^3.6.2",
    "body-parser": "^1.19.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.17.2",
    "graphql": "^15.3.0",
    "hsd": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.41",
    "typeorm-naming-strategies": "^2.0.0"
  }
}
