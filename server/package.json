{
  "name": "hns-stake-server",
  "version": "0.0.0",
  "main": "src/server.ts",
  "license": "MIT",
  "engines": {
    "node": ">=14.8.0"
  },
  "scripts": {
    "ts": "ts-node-dev -r tsconfig-paths/register -r dotenv/config --respawn --transpile-only --ignore-watch node_modules --no-notify",
    "start": "yarn ts src/server.ts",
    "start:producer": "yarn ts src/queue/producer.ts",
    "start:consumer": "yarn ts src/queue/consumer.ts",
    "build": "babel src --extensions \".ts\" --out-dir dist --copy-files --no-copy-ignored",
    "test": "jest",
    "docker:db:start": "docker run -d --name hns-stake-db -e POSTGRES_PASSWORD=docker -e POSTGRES_DB=hns-stake -p 5432:5432 postgres:10.15",
    "docker:redis:start": "docker run -d --name hns-stake-redis -p 6379:6379 redis:6.2-alpine",
    "docker:hsd:start": "docker run --name hsd -p 14037:14037 -p 14038:14038 -p 14039:14039 sinpapeles/hsd:2.4.0 --network regtest --http-host 0.0.0.0 --api-key=api-key --wallet-http-host=0.0.0.0 --listen",
    "db:seed": "NODE_PATH=./src ts-node -r dotenv/config src/db/seed.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/body-parser": "^1.19.2",
    "@types/bull": "^3.15.7",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.15",
    "@types/typescript": "^2.0.0",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv": "^14.3.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@bull-board/api": "^3.9.4",
    "@bull-board/express": "^3.9.4",
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "^3.6.2",
    "bfilter": "^1.0.5",
    "body-parser": "^1.19.1",
    "bull": "^4.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "debug": "^4.3.3",
    "express": "^4.17.2",
    "graphql": "^15.3.0",
    "hs-client": "^0.0.10",
    "hsd": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "sequelize": "^6.15.0",
    "sequelize-typescript": "^2.1.2",
    "type-graphql": "^1.1.1"
  }
}
